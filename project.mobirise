        {
          "alias": false,
          "_styles": {
            "& when not (@fullScreen)": {
              "padding-top": "(@paddingTop * 1rem)",
              "padding-bottom": "(@paddingBottom * 1rem)"
            },
            "overflow": "hidden",
            "& when (@bg-type = \"color\")": {
              "background-color": "@bg-value"
            },
            "& when (@bg-type = \"image\")": {
              "background-image": "url(@bg-value)",
              "& when (@overlay)": {
                ".mbr-overlay": {
                  "background": "@overlayColor",
                  "opacity": "@overlayOpacity"
                }
              }
            },
            ".title-wrapper": {
              "margin-bottom": "60px",
              "@media (max-width: 992px)": {
                "margin-bottom": "48px"
              },
              ".desc-wrapper": {
                ".mbr-desc": {
                  "display": "inline-block",
                  "width": "50%",
                  "margin-bottom": "36px",
                  "@media (max-width: 992px)": {
                    "width": "100%",
                    "margin-bottom": "16px"
                  }
                }
              },
              ".mbr-section-title": {
                "margin-bottom": "24px"
              },
              ".text-wrapper": {
                ".mbr-text": {
                  "display": "inline-block",
                  "width": "50%",
                  "margin-bottom": "0",
                  "@media (max-width: 992px)": {
                    "width": "100%"
                  }
                }
              }
            },
            ".embla__slide": {
              "display": "flex",
              "justify-content": "center",
              "position": "relative",
              "min-width": "850px",
              "max-width": "850px",
              "@media (max-width: 1200px)": {
                "min-width": "650px",
                "max-width": "650px"
              },
              "@media (max-width: 767px)": {
                "min-width": "100%",
                "max-width": "100%"
              },
              "&:hover, &:focus": {
                ".slide-content .item-wrapper .item-img img": {
                  "border": "2px solid @active"
                }
              },
              ".slide-content": {
                "width": "100%",
                ".item-wrapper": {
                  ".item-img": {
                    "margin-bottom": "32px",
                    "img": {
                      "height": "480px",
                      "width": "100%",
                      "object-fit": "cover",
                      "border": "2px solid transparent",
                      "@media (max-width: 992px)": {
                        "height": "350px"
                      }
                    }
                  },
                  ".item-content": {
                    "position": "relative",
                    "z-index": "1",
                    "padding": "2px",
                    ".title-wrap": {
                      ".item-title": {
                        "display": "inline-block",
                        "width": "50%",
                        "margin-bottom": "8px",
                        "@media (max-width: 992px)": {
                          "width": "100%"
                        }
                      }
                    },
                    ".text-wrap": {
                      ".item-text": {
                        "display": "inline-block",
                        "width": "50%",
                        "margin-bottom": "0",
                        "@media (max-width: 992px)": {
                          "width": "100%"
                        }
                      }
                    }
                  }
                }
              }
            },
            ".embla__button--next, .embla__button--prev": {
              "display": "flex"
            },
            ".embla__button": {
              "bottom": "0",
              "width": "48px",
              "height": "48px",
              "font-size": "24px",
              "background-color": "@arrowsBg !important",
              "color": "@arrowsColor !important",
              "border": "none !important",
              "border-radius": "50%",
              "transition": "all 0.3s",
              "position": "absolute",
              "display": "flex",
              "justify-content": "center",
              "align-items": "center",
              "opacity": "1 !important",
              "& when not (@arrows)": {
                "display": "none"
              },
              "& when not (@arrowsMobile)": {
                "@media (max-width: 768px)": {
                  "display": "none"
                }
              },
              "&:hover": {
                "transform": "scale(1.05)"
              },
              "&.embla__button--prev": {
                "left": "0"
              },
              "&.embla__button--next": {
                "right": "0"
              }
            },
            ".embla": {
              "position": "relative",
              "margin-bottom": "50px",
              "@media (max-width: 992px)": {
                "margin-bottom": "38px"
              },
              "& when (@arrows)": {
                "padding-bottom": "4rem"
              }
            },
            ".embla__viewport": {
              "width": "100%",
              "margin-right": "1rem"
            },
            ".embla__viewport.is-draggable": {
              "cursor": "grab"
            },
            ".embla__viewport.is-dragging": {
              "cursor": "grabbing"
            },
            ".embla__container": {
              "display": "flex",
              "user-select": "none",
              "-webkit-touch-callout": "none",
              "-khtml-user-select": "none",
              "-webkit-tap-highlight-color": "transparent"
            },
            ".item-menu-overlay": {
              "opacity": "0 !important"
            },
            ".mbr-section-title": {
              "color": "#ffffff"
            },
            ".mbr-text, .text-wrapper": {
              "color": "#9aa0a6",
              "text-align": "center"
            },
            ".mbr-desc, .desc-wrapper": {
              "color": "#9aa0a6",
              "text-align": "center"
            },
            ".item-title, .title-wrap": {
              "color": "#ffffff",
              "text-align": "center"
            },
            ".item-text, .text-wrap": {
              "color": "#9aa0a6",
              "text-align": "center"
            },
            ".mbr-section-title, .mbr-section-btn": {
              "text-align": "center"
            }
          },
          "_name": "slider02",
          "_sourceTheme": "progressm5",
          "_customHTML": "<section data-bs-version=\"5.1\" class=\"slider02 mbr-embla progressm5\" group=\"Gallery &amp; Slider\" plugins=\"embla\" mbr-class=\"{\n    'mbr-fullscreen': fullScreen,\n    'mbr-parallax-background': bg.parallax}\">\n    <mbr-parameters>\n        <header>Size</header>\n        <input type=\"checkbox\" title=\"Full Screen\" name=\"fullScreen\">\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\">\n        <input type=\"range\" inline=\"\" title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n        <input type=\"range\" inline=\"\" title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n        <input type=\"checkbox\" title=\"Reverse\" name=\"reverseContent\">\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" name=\"showTitle\" title=\"Title\" checked=\"\">\n        <input type=\"checkbox\" title=\"Text\" name=\"showText\" checked=\"\">\n        <input type=\"checkbox\" title=\"Description\" name=\"showDesc\" checked=\"\">\n        <input type=\"checkbox\" title=\"Buttons\" name=\"showButtons\" checked=\"\">\n        <header>Carousel</header>\n        <input type=\"checkbox\" title=\"Autoplay\" name=\"autoplay\">\n        <input type=\"range\" title=\"Interval\" name=\"interval\" min=\"1\" max=\"10\" value=\"4\" step=\"1\" condition=\"autoplay\">\n        <input type=\"checkbox\" title=\"Loop\" name=\"loop\" checked=\"\">\n        <input type=\"checkbox\" title=\"Draggable\" name=\"draggable\" checked=\"\">\n        <input type=\"checkbox\" name=\"arrows\" title=\"Arrows\" checked=\"\">\n        <input type=\"checkbox\" name=\"arrowsMobile\" title=\"Mobile Arrows\" condition=\"arrows\" checked=\"\">\n        <input type=\"color\" name=\"arrowsBg\" title=\"Arrows BG\" value=\"#252525\" condition=\"arrows\">\n        <input type=\"color\" name=\"arrowsColor\" title=\"Arrows Color\" value=\"#ffffff\" condition=\"arrows\">\n        <header>Cards</header>\n        <input type=\"range\" name=\"gap\" title=\"Gap\" min=\"0\" max=\"10\" step=\"1\" value=\"0\">\n        <input type=\"checkbox\" name=\"showCardTitle\" title=\"Card Title\" checked=\"\">\n        <input type=\"checkbox\" name=\"showCardText\" title=\"Card Text\" checked=\"\">\n        <input type=\"color\" name=\"active\" title=\"Active Color\" value=\"#078efb\">\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\" parallax=\"\" required=\"\">\n            <input type=\"image\" title=\"Image\" value=\"@PROJECT_PATH@/assets/images/background1.jpg\">\n            <input type=\"color\" title=\"Color\" value=\"#060606\" selected=\"\">\n        </fieldset>\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked=\"\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#060606\" condition=\"overlay &amp;&amp; bg.type == 'image'\">\n        <input type=\"range\" inline=\"\" title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.1\" condition=\"overlay &amp;&amp; bg.type == 'image'\">\n    </mbr-parameters>\n    <div class=\"mbr-overlay\" mbr-if=\"overlay &amp;&amp; bg.type !== 'color'\"></div>\n\n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        <div class=\"row\">\n            <div class=\"col-12\">\n                <div class=\"content-wrapper\">\n                    <div class=\"title-wrapper\" mbr-if=\"showTitle || showText || showDesc\">\n                        <div class=\"desc-wrapper\">\n                            <p class=\"mbr-desc mbr-fonts-style\" mbr-theme-style=\"display-4\" mbr-if=\"showDesc\" data-app-selector=\".mbr-desc, .desc-wrapper\">\n                                MODERN LIVING SPACES\n                            </p>\n                        </div>\n                        <h2 class=\"mbr-section-title mbr-fonts-style\" data-app-selector=\".mbr-section-title, .mbr-section-btn\" mbr-theme-style=\"display-2\" mbr-if=\"showTitle\">\n                            Smart home setups\n                        </h2>\n                        <div class=\"text-wrapper\">\n                            <p class=\"mbr-text mbr-fonts-style\" mbr-theme-style=\"display-4\" mbr-if=\"showText\" data-app-selector=\".mbr-text, .text-wrapper\">\n                                Smart home setups have evolved to cater to various needs and preferences, offering a\n                                wide range of configurations that enhance convenience, security, and efficiency. These\n                                setups integrate multiple smart devices and systems, creating a cohesive environment\n                                where technology seamlessly interacts with daily life.\n                            </p>\n                        </div>\n                    </div>\n                    <div class=\"slider-wrapper\">\n                        <div class=\"embla\" data-loop=\"{{loop}}\" data-auto-play=\"{{autoplay}}\" data-auto-play-interval=\"{{interval}}\" data-draggable=\"{{draggable}}\" data-skip-snaps=\"true\" data-align=\"center\" data-contain-scroll=\"trimSnaps\">\n                            <div class=\"embla__viewport\">\n                                <div class=\"embla__container\">\n                                    <div class=\"embla__slide slider-image item\" mbr-style=\"{'margin-left': gap + 'rem', 'margin-right': gap + 'rem'}\">\n                                        <div class=\"slide-content\">\n                                            <div class=\"item-wrapper\" mbr-class=\"{'flex-row-reverse' : reverseContent}\">\n                                                <div class=\"item-img\">\n                                                    <img src=\"@PROJECT_PATH@/assets/images/image2.jpg\" alt=\"Mobirise Website Builder\">\n                                                </div>\n                                                <div class=\"item-content\">\n                                                    <div class=\"title-wrap\">\n                                                        <h4 class=\"item-title mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".item-title, .title-wrap\" mbr-if=\"showCardTitle\">\n                                                            Basic smart home setup\n                                                        </h4>\n                                                    </div>\n                                                    <div class=\"text-wrap\">\n                                                        <p class=\"item-text mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".item-text, .text-wrap\" mbr-if=\"showCardText\">\n                                                            Voice-activated control for basic functions.\n                                                            Remote control via smartphone apps.\n                                                            Simple automation routines (e.g., lights turning on/off at\n                                                            specific times).\n                                                        </p>\n                                                    </div>\n                                                </div>\n                                            </div>\n                                        </div>\n                                    </div>\n                                    <div class=\"embla__slide slider-image item\" mbr-style=\"{'margin-left': gap + 'rem', 'margin-right': gap + 'rem'}\">\n                                        <div class=\"slide-content\">\n                                            <div class=\"item-wrapper\" mbr-class=\"{'flex-row-reverse' : reverseContent}\">\n                                                <div class=\"item-img\">\n                                                    <img src=\"@PROJECT_PATH@/assets/images/image4.jpg\" alt=\"Mobirise Website Builder\">\n                                                </div>\n                                                <div class=\"item-content\">\n                                                    <div class=\"title-wrap\">\n                                                        <h4 class=\"item-title mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".item-title, .title-wrap\" mbr-if=\"showCardTitle\">\n                                                            Security-focused setup\n                                                        </h4>\n                                                    </div>\n                                                    <div class=\"text-wrap\">\n                                                        <p class=\"item-text mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".item-text, .text-wrap\" mbr-if=\"showCardText\">\n                                                            Real-time video surveillance and recording.\n                                                            Remote monitoring and notifications for suspicious activity.\n                                                            Keyless entry and remote lock/unlock functionality.\n                                                        </p>\n                                                    </div>\n                                                </div>\n                                            </div>\n                                        </div>\n                                    </div>\n                                    <div class=\"embla__slide slider-image item\" mbr-style=\"{'margin-left': gap + 'rem', 'margin-right': gap + 'rem'}\">\n                                        <div class=\"slide-content\">\n                                            <div class=\"item-wrapper\" mbr-class=\"{'flex-row-reverse' : reverseContent}\">\n                                                <div class=\"item-img\">\n                                                    <img src=\"@PROJECT_PATH@/assets/images/image13.jpg\" alt=\"Mobirise Website Builder\">\n                                                </div>\n                                                <div class=\"item-content\">\n                                                    <div class=\"title-wrap\">\n                                                        <h4 class=\"item-title mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".item-title, .title-wrap\" mbr-if=\"showCardTitle\">\n                                                            Energy-efficient setup\n                                                        </h4>\n                                                    </div>\n                                                    <div class=\"text-wrap\">\n                                                        <p class=\"item-text mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".item-text, .text-wrap\" mbr-if=\"showCardText\">\n                                                            Automated heating and cooling based on occupancy and\n                                                            weather.\n                                                            Energy-saving lighting with motion sensors and dimming\n                                                            capabilities.\n                                                            Monitoring and optimizing power usage of various devices.\n                                                        </p>\n                                                    </div>\n                                                </div>\n                                            </div>\n                                        </div>\n                                    </div>\n                                    <div class=\"embla__slide slider-image item\" mbr-style=\"{'margin-left': gap + 'rem', 'margin-right': gap + 'rem'}\">\n                                        <div class=\"slide-content\">\n                                            <div class=\"item-wrapper\" mbr-class=\"{'flex-row-reverse' : reverseContent}\">\n                                                <div class=\"item-img\">\n                                                    <img src=\"@PROJECT_PATH@/assets/images/image15.jpg\" alt=\"Mobirise Website Builder\">\n                                                </div>\n                                                <div class=\"item-content\">\n                                                    <div class=\"title-wrap\">\n                                                        <h4 class=\"item-title mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".item-title, .title-wrap\" mbr-if=\"showCardTitle\">\n                                                            Comprehensive smart home setup\n                                                        </h4>\n                                                    </div>\n                                                    <div class=\"text-wrap\">\n                                                        <p class=\"item-text mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".item-text, .text-wrap\" mbr-if=\"showCardText\">\n                                                            Centralized control of all smart home devices.\n                                                            Complex automation routines and scenarios.\n                                                            Integration with various smart home ecosystems and platforms.\n                                                        </p>\n                                                    </div>\n                                                </div>\n                                            </div>\n                                        </div>\n                                    </div>\n                                    <div class=\"embla__slide slider-image item\" mbr-style=\"{'margin-left': gap + 'rem', 'margin-right': gap + 'rem'}\">\n                                        <div class=\"slide-content\">\n                                            <div class=\"item-wrapper\" mbr-class=\"{'flex-row-reverse' : reverseContent}\">\n                                                <div class=\"item-img\">\n                                                    <img src=\"@PROJECT_PATH@/assets/images/image3.jpg\" alt=\"Mobirise Website Builder\">\n                                                </div>\n                                                <div class=\"item-content\">\n                                                    <div class=\"title-wrap\">\n                                                        <h4 class=\"item-title mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".item-title, .title-wrap\" mbr-if=\"showCardTitle\">\n                                                            Entertainment and media setup\n                                                        </h4>\n                                                    </div>\n                                                    <div class=\"text-wrap\">\n                                                        <p class=\"item-text mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".item-text, .text-wrap\" mbr-if=\"showCardText\">\n                                                            Voice-controlled entertainment systems.\n                                                            High-quality audio and video streaming.\n                                                            Integration with streaming services and content providers.\n                                                        </p>\n                                                    </div>\n                                                </div>\n                                            </div>\n                                        </div>\n                                    </div>\n                                </div>\n                            </div>\n                            <button class=\"embla__button embla__button--prev\">\n                                <span class=\"mobi-mbri mobi-mbri-left mbr-iconfont\" aria-hidden=\"true\"></span>\n                                <span class=\"sr-only visually-hidden visually-hidden visually-hidden\">Previous</span>\n                            </button>\n                            <button class=\"embla__button embla__button--next\">\n                                <span class=\"mobi-mbri mobi-mbri-right mbr-iconfont\" aria-hidden=\"true\"></span>\n                                <span class=\"sr-only visually-hidden visually-hidden visually-hidden\">Next</span>\n                            </button>\n                        </div>\n                    </div>\n                    <div mbr-buttons=\"\" mbr-theme-style=\"display-4\" class=\"mbr-section-btn\" mbr-if=\"showButtons\" data-toolbar=\"-mbrBtnMove\">\n                        <a class=\"btn btn-secondary\" href=\"https://mobiri.se\" data-app-placeholder=\"Type Text\">\n                            <span class=\"mobi-mbri mobi-mbri-arrow-next mbr-iconfont mbr-iconfont-btn\"></span>\n                            Get started now\n                        </a>\n                        <a class=\"btn btn-success\" href=\"https://mobiri.se\" data-app-placeholder=\"Type Text\">\n                            <span class=\"mobi-mbri mobi-mbri-arrow-next mbr-iconfont mbr-iconfont-btn\"></span>\n                            View more setups\n                        </a>\n                    </div>\n                </div>\n            </div>\n        </div>\n    </div>\n</section>\n",
